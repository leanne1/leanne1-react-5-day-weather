{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
        "mocha": true,
    },
    "rules": {
        "no-shadow": 1,
        "comma-dangle": [2, "always-multiline"],
        "indent": [2, "tab", { "SwitchCase": 1 }],
        "strict": [1, "global"],
        "no-var": 0,
        "no-param-reassign": 0,
        "no-unused-vars": 0,
        "prefer-const": 0,
        "space-before-blocks": 0,
        "func-names": 0,
        "react/jsx-boolean-value": 0,
        "jsx-quotes": [2, "prefer-single"],
        "camelcase": 0,
        "no-unused-expressions": 0,
        "default-case": 0,
        "no-else-return": 0,
        "no-nested-ternary": 0,
        "new-cap": [2, { "capIsNewExceptions": ["Intl", "Map","SendData","Router","FormattedHTMLMessagereturn"] }],
    }
}
